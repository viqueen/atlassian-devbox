{
  "name": "atlassian-devbox",
  "version": "1.0.1",
  "description": "atlassian devbox",
  "preferGlobal": true,
  "bin": {
    "bamboo": "dist/cli/bamboo.js",
    "bitbucket": "dist/cli/bitbucket.js",
    "confluence": "dist/cli/confluence.js",
    "crowd": "dist/cli/crowd.js",
    "ctk-server": "dist/cli/ctk-server.js",
    "fecru": "dist/cli/fecru.js",
    "jira": "dist/cli/jira.js"
  },
  "scripts": {
    "build": "tsc --watch",
    "ci:build": "rimraf ./dist && tsc",
    "format:check": "prettier --check src",
    "format": "prettier --check --write src",
    "postinstall": "node post-install.js",
    "check-updates": "npm-check-updates -u"
  },
  "files": [
    "dist",
    ".atlassian-devbox",
    "post-install.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viqueen/atlassian-devbox.git"
  },
  "keywords": [
    "atlassian",
    "devbox"
  ],
  "author": "Hasnae R.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/viqueen/atlassian-devbox/issues"
  },
  "homepage": "https://github.com/viqueen/atlassian-devbox#readme",
  "devDependencies": {
    "@types/node": "^17.0.13",
    "husky": "^7.0.4",
    "npm-check-updates": "^12.2.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "commander": "^9.0.0",
    "fs-extra": "^10.0.1"
  }
}

{
  "name": "atlassian-devbox",
  "version": "1.0.0",
  "description": "atlassian devbox",
  "preferGlobal": true,
  "bin": {
    "bamboo": "dist/cli/bamboo.js",
    "bitbucket": "dist/cli/bitbucket.js",
    "confluence": "dist/cli/confluence.js",
    "crowd": "dist/cli/crowd.js",
    "jira": "dist/cli/jira.js"
  },
  "scripts": {
    "build": "tsc --watch",
    "ci:build": "rimraf ./dist && tsc",
    "format:check": "prettier --check src",
    "format": "prettier --check --write src",
    "prepare": "husky install",
    "postinstall": "node post-install.js"
  },
  "files": [
    "dist",
    ".atlassian-devbox",
    "post-install.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viqueen/atlassian-devbox.git"
  },
  "keywords": [
    "atlassian",
    "devbox"
  ],
  "author": "Hasnae R.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/viqueen/atlassian-devbox/issues"
  },
  "homepage": "https://github.com/viqueen/atlassian-devbox#readme",
  "devDependencies": {
    "@types/node": "^15.14.0",
    "husky": "^7.0.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "commander": "^8.0.0",
    "fs-extra": "^10.0.0"
  }
}

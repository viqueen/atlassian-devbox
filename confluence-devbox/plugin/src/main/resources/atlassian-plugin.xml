<atlassian-plugin key="${atlassian.plugin.key}" name="${project.name}" plugins-version="2">
    <plugin-info>
        <description>${project.description}</description>
        <version>${project.version}</version>
        <vendor name="${project.organization.name}" url="${project.organization.url}" />
    </plugin-info>

    <rest key="confluence-devbox-rest-resources"
          name="Confluence Devbox Rest Resources"
          path="/devbox"
          version="1.0">
        <package>org.viqueen.atlassian.confluence.rest</package>
    </rest>

    <web-item
            key="confluence-web-item-plugin-module"
            section="system.user/user-operations"
            weight="1"
    >
        <label>WebUI Module</label>
        <link>https://developer.atlassian.com/server/confluence/web-ui-modules/</link>
    </web-item>

</atlassian-plugin>
